---
import Samples from '@/components/Samples.astro'
import SectionContainer from '@/components/SectionContainer.astro'
import products from '@/data/products.json'
interface Texts {
  title: string
  cta: string
}

interface Props {
  texts: Texts
}

const { texts } = Astro.props
const standardGallery = {
  images: {},
  texts: {
    title: texts.title,
    callToAction: texts.cta,
  },
  products,
  href: '/tienda',
}
---

<SectionContainer variant="darker">
  <Samples {...standardGallery} />
</SectionContainer>
