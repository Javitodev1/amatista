---
import filters from '@/data/filters.json'
---

<ul class="text-gray-600 dark:text-gray-400 [&>li]:mb-4">
  {
    filters.map((filter) => (
      <li>
        <a
          href={`/tienda/${filter}`}
          class="capitalize hover:underline"
        >
          {filter}
        </a>
      </li>
    ))
  }
</ul>
